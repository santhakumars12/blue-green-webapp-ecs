{
    "executionRoleArn": "arn:aws:iam::389560765612:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "nginx-website",
            "image": "389560765612.dkr.ecr.us-west-2.amazonaws.com/webapp:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "blue-green-family"
}